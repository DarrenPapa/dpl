#cmd: python3 /storage/emulated/0/dpl/dpl.py -skip-non-essential -no-cffi -no-lupa -init-time -simple-run $file
&use {std/text_io.py}

# Pure DPL runs faster since we just interpret it.
fn fib (n)
    set a 1
    set b 0
    set c 0
    while [:n> -1]
        set c [:a+:b]
        set a :b
        set b :c
        set n [:n - 1]
    end
    return :a
end
START_TIME
catch [!result] fib (10000)
STOP_TIME
LOG_TIME